<div id="course_<%= course.id %>" class="course col-md-4">
	<%= link_to course do %><img src="<%= course.image %>" alt="course_<%= course.id %> image" class="img-responsive img-thumbnail"><% end %>
	<h4><%= link_to course.name, course %></h4>
	<p><%= course.category.name %></p>
	<p><%= course.city_name %></p>
	<br>
	<br>
</div>
